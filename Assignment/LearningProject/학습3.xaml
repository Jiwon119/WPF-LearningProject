<UserControl x:Class="LearningProject.학습3"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LearningProject"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="g">
        <Grid x:Name="col">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Button x:Name="init" Width="70" Height="50" Content="초기화면" Grid.Column="3" Margin="120,90,10,0" Grid.Row="2" VerticalAlignment="Top" Click="init_Click"/>
            <Button x:Name="button1" Grid.Column="1" Grid.Row="1" Margin="10" Content="람다식"/>
            <Button x:Name="button2" Grid.Column="2" Grid.Row="1" Margin="10" Content="함수식" Click="btn_Click" />
        </Grid>
    </Grid>
</UserControl>
