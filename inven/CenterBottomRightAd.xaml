<UserControl x:Class="inven.CenterBottomRightAd"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:inven"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="CenterBottomRight_Ad1" Click="Btn_click" Grid.Row="0">
            <Image Source="img/UC6/2.jpg" Stretch="Fill" Margin="0,0,0,3"></Image>
        </Button>
        <Button x:Name="CenterBottomRight_Ad2" Click="Btn_click" Grid.Row="1">
            <Image Source="img/UC6/1.jpg" Stretch="Fill" Margin="0,3,0,0"></Image>
        </Button>
    </Grid>
</UserControl>
